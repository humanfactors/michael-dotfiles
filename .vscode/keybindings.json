// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+k ctrl+b",
        "command": "bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+k",
        "command": "-bookmarks.toggle",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+oem_6",
        "command": "bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+l",
        "command": "-bookmarks.jumpToNext",
        "when": "editorTextFocus"
    },
    {
        "key": "shift+alt+oem_4",
        "command": "bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+j",
        "command": "-bookmarks.jumpToPrevious",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl-shift-m",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "%>%"
        },
        "when": "editorLangId == 'r'"
    },
    {
        "key": "ctrl-'",
        "command": "editor.action.insertSnippet",
        "args": {
            "snippet": "- [ ] "
        },
        "when": "editorLangId == 'markdown'"
    },
    {
        "key": "ctrl+k ctrl+e",
        "command": "workbench.action.quickOpen"
    },
    {
        "key": "ctrl-e",
        "command": "cursorEnd",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl-a",
        "command": "cursorHome",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+a",
        "command": "editor.action.selectAll"
    },
    {
        "key": "ctrl+alt+a",
        "command": "editor.action.selectAll"
    }, 
    {
        "key": "ctrl+k ctrl-a",
        "command": "workbench.action.tasks.runTask"
    },
    { 
        "key": "ctrl+j ctrl+j",
        "command": "cursorDown",
        "when": "editorTextFocus" 
    },
    { 
        "key": "ctrl+j ctrl+n",
        "command": "cursorDown",
        "when": "editorTextFocus" 
    },
    { 
        "key": "ctrl+j ctrl+k",
        "command": "cursorUp",
        "when": "editorTextFocus" 
    },
    { 
        "key": "ctrl+j ctrl+p",
        "command": "cursorUp",
        "when": "editorTextFocus" 
    }
]
